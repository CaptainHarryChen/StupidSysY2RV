fun @main(): i32 {
%entry_main:
  @a = alloc i32
  store 1, @a
  @b = alloc i32
  store 2, @b
  %temp = alloc i32
  store 1, %temp
  %0 = load @a
  %1 = eq %0, 0
  br %1, %true, %end

%true:
  %2 = load @b
  %3 = ne %2, 0
  store %3, %temp
  jump %end

%end:
  %4 = load %temp
  br %4, %true_0, %false

%true_0:
  %5 = load @a
  %6 = load @b
  %7 = add %5, %6
  store %7, @a
  jump %end_0

%end_0:
  %8 = load @a
  ret %8

%false:
  jump %end_0
}
